<?xml version="1.0" encoding="UTF-8"?>


<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
 "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
 
 <mapper namespace="com.shinnal.spring.test.ajax.repository.BookingRepository">
 
 	<insert id="insertBooking" parameterType="com.shinnal.spring.test.ajax.domain.Booking">
 		
 		INSERT INTO
 			`booking`
 		(
 			`name`
 			, `headcount`
 			, `day`
 			, `date`
 			, `phoneNumber`
 		)	
 		VELUE
 		(
 			#{name}
 			, #{headcount}
 			, #{day}
 			, #{date}
 			, #{phoneNumber}
 		)
 	</insert>
 	
 	<select id="selectBooking" resultType="com.shinnal.spring.test.ajax.domain.Booking">
 		SELECT
 			`id`
 			, `name`
 			, `date`
 			, `day`
 			, `headcount`
 			, `phoneNumber`
 			, `state`
 			, `createdAt`
 			, `updatedAt`
 		FROM
 			`booking`
 		
 	</select>
 
 
 
 </mapper>